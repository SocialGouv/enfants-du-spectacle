{
  "extends": ["next/core-web-vitals", "prettier", "eslint-config-next"],
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "."]
      }
    }
  },
  "rules": {
    "@typescript-eslint/naming-convention": "off" // TS linter suggests camelCase for React Components
  }
}
