{
  "name": "enfants-du-spectacle",
  "version": "1.15.7",
  "private": true,
  "release": {
    "branches": [
      "main",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  },
  "scripts": {
    "dev": "next dev",
    "dev-procfile": "OVERMIND_NO_PORT=1 overmind start -f Procfile.dev",
    "build": "next build --no-lint",
    "start": "next start",
    "start-prod": "export NEXTAUTH_URL=$APP_BASE_URL && npx prisma migrate deploy && next start",
    "start-review": "export NEXTAUTH_URL=$APP_BASE_URL && npx prisma migrate deploy && source src/scripts/restore_seeds.sh && next start",
    "check-types": "tsc --noemit",
    "lint": "next lint",
    "prepare": "husky install",
    "send-test-mail": "node scripts/sendTestMail.js"
  },
  "dependencies": {
    "@dataesr/react-dsfr": "^0.9.2",
    "@next-auth/prisma-adapter": "^1.0.0",
    "@prisma/client": "^3.6.0",
    "@sentry/nextjs": "^6.15.0",
    "date-fns": "^2.27.0",
    "email-addresses": "^5.0.0",
    "hoist-non-react-statics": "^3.3.2",
    "javascript-state-machine": "^3.1.0",
    "lodash": "^4.17.21",
    "next": "^11.1.2",
    "next-auth": "4.0.2",
    "nodemailer": "^6.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.3.1",
    "remixicon": "^2.5.0",
    "sass": "^1.44.0",
    "superjson": "^1.8.0",
    "swr": "^1.1.0",
    "use-debounce": "^7.0.1"
  },
  "devDependencies": {
    "@socialgouv/eslint-config-react": "^1.100.0",
    "@socialgouv/eslint-config-recommended": "^1.100.0",
    "@socialgouv/eslint-config-typescript": "^1.100.0",
    "@types/faker": "^5.5.9",
    "@types/react": "17.0.37",
    "csv-parse": "^5.0.3",
    "csv-stringify": "^6.0.3",
    "eslint": "^7.32.0",
    "eslint-config-next": "12.0.4",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "maildev": "^1.1.0",
    "mjml": "^4.11.0",
    "prettier": "^2.5.1",
    "prisma": "^3.6.0",
    "slugify": "^1.6.3",
    "ts-node": "^10.4.0",
    "typescript": "4.5.2"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "npx eslint --cache"
  }
}
